<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/routeFrame"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ExtraText">

    <FrameLayout
        android:id="@+id/frame_rp_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="70dp"
        app:layout_constraintTop_toBottomOf="@+id/NavViewRutePlace">

    </FrameLayout>

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/NavViewRutePlace"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/colorBackDark"
        app:itemIconTint="@color/nav_item_color_select"
        android:clickable="true"
        android:focusable="true"
        app:elevation="0dp"
        app:itemIconSize="30dp"
        app:itemTextColor="@color/nav_item_color_select"
        app:menu="@menu/navigationbarroutemenu" />

</FrameLayout>